{
    "name": "Swisscows.VPN",
    "version": "1.1.6",
    "description": "This browser extension simplifies access to the Swisscows.VPN server.",
    "icons": {
        "16": "favicon-16.png",
        "32": "favicon-32.png",
        "48": "favicon-48.png",
        "128": "favicon-128.png"
    },
    "permissions": [
        "proxy",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],

    "browser_action": {
        "default_icon": {
            "16": "favicon-16.png",
            "32": "favicon-32.png"
        },
        "default_title": "Swisscows.VPN",
        "default_popup": "popup.html"

    },
    "options_page": "options.html",
    "background": {
        "scripts": [
            "js/authentication.js",
            "js/background.js"
        ],
        "persistent": true
    },
    "default_locale": "en",
    "manifest_version": 2
}